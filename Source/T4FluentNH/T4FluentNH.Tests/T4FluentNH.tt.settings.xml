<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  <BaseEntityType>T4FluentNH.Domain.IEntity</BaseEntityType>
  <EntityTypes>
    <EntityType>
      <DerivedFromType>T4FluentNH.Tests.CodeList</DerivedFromType>
      <IdentifierPropertyName>Code</IdentifierPropertyName>
      <IdentifierPropertyType>System.String</IdentifierPropertyType>
      <IdentifierLength>20</IdentifierLength>
      <AttributeIdentifierLengths>
        <AttributeIdentifierLength>
          <Type>T4FluentNH.Tests.IdentifierLengthAttributeByProperty</Type>
          <PropertyName>Length</PropertyName>
        </AttributeIdentifierLength>
        <AttributeIdentifierLength>
          <Type>T4FluentNH.Tests.IdentifierLengthAttributeByIndex</Type>
          <ParameterIndex>1</ParameterIndex>
        </AttributeIdentifierLength>
      </AttributeIdentifierLengths>
      <CustomIdentifierLengths>
        <CustomIdentifierLength>
          <Type>T4FluentNH.Tests.EntityTypes.ETLengthCodeList</Type>
          <Length>30</Length>
        </CustomIdentifierLength>
      </CustomIdentifierLengths>
      <KeyColumn>
        <Prefix></Prefix>
        <Postfix>Code</Postfix>
      </KeyColumn>
    </EntityType>
    <EntityType>
      <DerivedFromType>T4FluentNH.Domain.Entity</DerivedFromType>
      <IdentifierPropertyName>Id</IdentifierPropertyName>
      <IdentifierPropertyType>System.Int32</IdentifierPropertyType>
      <KeyColumn>
        <Prefix></Prefix>
        <Postfix>Id</Postfix>
      </KeyColumn>
    </EntityType>
    <EntityType>
      <DerivedFromType>T4FluentNH.Domain.IUser</DerivedFromType>
      <IdentifierPropertyName>Id</IdentifierPropertyName>
      <IdentifierPropertyType>System.Int32</IdentifierPropertyType>
      <KeyColumn>
        <Prefix></Prefix>
        <Postfix>Id</Postfix>
      </KeyColumn>
    </EntityType>
  </EntityTypes>
  <Strict>False</Strict>
  <SyntheticProperties>
    <Generate>True</Generate>
    <Comment>Test</Comment>
    <AddNotNullAttribute>False</AddNotNullAttribute>
    <AddLengthAttribute>True</AddLengthAttribute>
    <CustomAttributes>
      <CustomAttribute>ReadOnly(true)</CustomAttribute>
    </CustomAttributes>
    <GenerateForDeriveredTypes>True</GenerateForDeriveredTypes> <!--will generate synthetic props for derivered types if they haven't one-->
  </SyntheticProperties>
  <Singularization>
    <Singularize>
      <Pattern>^([\w]*)Uses$</Pattern>
      <Replace>$1Use</Replace>
    </Singularize>
  </Singularization>
  <GenerateMetadata>
      <Relations>true</Relations>
  </GenerateMetadata>
  <AsyncMethods>true</AsyncMethods> <!--Works only with NHibernateX-->
  <TypedMappingExtensions>true</TypedMappingExtensions>
  <ReadOnlyAttributeConvention>true</ReadOnlyAttributeConvention>
  <NotNullAttribute>T4FluentNH.Tests.NotNullAttribute</NotNullAttribute>
  <LengthAttribute>T4FluentNH.Tests.LengthAttribute</LengthAttribute>
  <DefaultCollectionMapType>Set</DefaultCollectionMapType>
  <IgnoreTypes>
    <!--<IgnoreType>T4FluentNH.Tests.OneToMany.M2OCar</IgnoreType>-->
  </IgnoreTypes>
  <ExternalAssemblies>
    <ExternalAssemblyPath>../T4FluentNH.Domain/bin/Debug/T4FluentNH.Domain.dll</ExternalAssemblyPath>
  </ExternalAssemblies>
</Settings>